# Frontend environment example
# Base API path used by axios httpClient
VITE_API_BASE_URL=/api

# Backend origin used for Vite dev proxy and resolveUrl in production
# e.g. http://localhost:8080 or https://api.example.com
VITE_BACKEND_ORIGIN=http://localhost:8080

# OAuth2 Configuration
# QQ App ID (获取地址: https://connect.qq.com/)
VITE_QQ_APP_ID=YOUR_QQ_APP_ID

# WeChat App ID (获取地址: https://open.weixin.qq.com/)
VITE_WECHAT_APP_ID=YOUR_WECHAT_APP_ID
